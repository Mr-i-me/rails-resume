<%= render 'shared/title', title: "Build your Resume" %>

<div class="row">
  <div class="medium-4 columns">
    <div class="panel">
      <h3>Skills</h3>
      <%= link_to "Add Skill", new_resume_skill_path(@resume), class: 'button secondary' %>
    </div>
  </div>
  <div class="medium-4 columns">
    <div class="panel">
      <h3>Education</h3>
      <%= link_to "Add School", new_resume_school_path(@resume), class: 'button secondary' %>
    </div>
  </div>
  <div class="medium-4 columns">
    <div class="panel">
      <h3>Work Experience</h3>
      <%= link_to "Add Job", new_resume_experience_path(@resume), class: 'button secondary' %>
    </div>
  </div>
</div>
<div class="row">
  <div class="medium-4 columns">
    <div class="panel">
      <h3>Hobbies</h3>
      <%= link_to "Add Hobby", new_resume_hobby_path(@resume), class: 'button secondary' %>
    </div>
  </div>
  <div class="medium-4 columns">
    <div class="panel">
      <h3>References</h3>
      <%= link_to "Add Reference", new_resume_reference_path(@resume), class: 'button secondary' %>
    </div>
  </div>
  <div class="medium-4 columns">
    <div class="panel">
      <h3>Custom</h3>
      <%= link_to "Add Custom", new_resume_custom_path(@resume), class: 'button secondary' %>
    </div>
  </div>
</div>
