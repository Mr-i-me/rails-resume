<%= render 'shared/title', title: "Resume Builder" %>
<%= render 'layouts/messages' %>
<div class="row">
  <div class="medium-6 columns">
    <h3>Recent Resumes</h3>
    <ul class="no-bullet">
      <%= render @resumes[0..4] %>
    </ul>
  </div>
  <div class="medium-6 columns panel">
    <h3>Create a Resume</h3>
    <%= render 'form' %>
  </div>
</div>
